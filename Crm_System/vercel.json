{
  "version": 2,
  "builds": [
    {
      "src": "Crm_System/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "50mb"
      }
    },
    {
      "src": "manage.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "Crm_System/wsgi.py"
    }
  ]
}